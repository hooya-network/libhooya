PROJECT_NAME           = "libhooya"
PROJECT_NUMBER         = "@CMAKE_PROJECT_VERSION@"
PROJECT_BRIEF          = "Gateway to interfacing with the HooYa! network"
PROJECT_LOGO           =
OPTIMIZE_OUTPUT_FOR_C  = YES
BUILTIN_STL_SUPPORT    = YES
HIDE_UNDOC_MEMBERS     = NO
HIDE_UNDOC_CLASSES     = NO
SHOW_USED_FILES        = NO
SHOW_FILES             = YES
WARNINGS               = YES
WARN_IF_UNDOCUMENTED   = YES
WARN_IF_DOC_ERROR      = YES
WARN_NO_PARAMDOC       = YES
WARN_AS_ERROR          = NO
INPUT = "@CRYPTO_DIR@" \
	"@NET_DIR@" \
	"@PIPELINE_DIR@" \
	"@SOCK_DIR@" \
	"@DOXYGEN_README_MD@"
FILE_PATTERNS          = *.c \
                         *.cc \
                         *.h \
                         *.hh \
                         *.txt
RECURSIVE              = YES

USE_MDFILE_AS_MAINPAGE = "@DOXYGEN_README_MD@"
SOURCE_BROWSER         = YES
STRIP_CODE_COMMENTS    = YES
REFERENCED_BY_RELATION = YES
REFERENCES_RELATION    = YES
REFERENCES_LINK_SOURCE = YES

GENERATE_HTML          = YES
HTML_OUTPUT            = html
HTML_FOOTER            = "@DOXYGEN_FOOTER@"
HTML_COLORSTYLE_HUE    = 240
HTML_COLORSTYLE_SAT    = 200
HTML_COLORSTYLE_GAMMA  = 50
HTML_TIMESTAMP         = YES
SEARCHENGINE           = YES

GENERATE_LATEX         = NO

HAVE_DOT               = YES
DOT_FONTSIZE           = 10
GRAPHICAL_HIERARCHY    = YES

CLASS_GRAPH            = YES
TEMPLATE_RELATIONS     = YES
CALL_GRAPH             = YES
CALLER_GRAPH           = YES


INTERACTIVE_SVG        = YES
DOT_TRANSPARENT        = YES
